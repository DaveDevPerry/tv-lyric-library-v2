<div class="page-header">
	<h2 class="page-title">all songs</h2>
	<div class="header-wrapper">
		<div class="blank"></div>
		<a href="/songs/new" class="artist-name">Add Song</a>
	</div>
</div>
<div class="page-content">
	<!-- <a class="btn btn-add" href="/songs/new">Add Song</a> -->

	<p class="instructions">
		select a song below, to review lyrics or add a new song from scratch
	</p>

	<div class="song-list-container">
		<% songs.forEach(song => { %>
		<a href="/songs/<%= song.id %>" class="song-row">
			<div class="song-title"><%= song.title %></div>
		</a>
		<% }) %>
	</div>
</div>
