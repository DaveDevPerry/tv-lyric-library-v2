<table id="songs-table">
	<thead>
		<!-- <th></th> -->
		<th class="left">song</th>
		<th><i class="far fa-edit"></i></th>
	</thead>
	<tbody>
		<% songs.forEach(song => { %>
		<tr>
			<td>
				<a href="/songs/<%= song.id %>">
					<div class="song-title"><%= song.title %></div>
				</a>
			</td>
			<td>
				<div class="song-review-count right"><%= song.reviewCount %></div>
			</td>
		</tr>
		<% }) %>
	</tbody>
</table>
